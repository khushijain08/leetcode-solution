class Solution {
    public boolean isPalindrome(int x) {
        int y=0,r,temp;
        temp=x;
        while(x>0)
        {
            r=x%10;
            y=y*10+r;
            x=x/10;
        }
        if(temp==y)
        {
            return true;
        }
            else
            {
                return false;
            }
    
 

    }
}
